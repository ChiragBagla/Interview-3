class hotel{

final static int MAX_KING 20;
final static int MAX_QUEEN 20;

Hashmap<room, boolean> kingsRoom;
Hashmap<room,boolean> queenssRoom;

boolean isFull(Hashmap room){
	if room.containsValue("true") return false;
	else return true;
}

boolean isFullHotel(){
	return (isFull(kingsRoom) && isFull(queensRoom));
}


}



abstract class room{
boolean isOccupied.
int price;
}

kingsRoom extends room{

}

queensRoom extends room{

}

reservedTime{
String start;
String end;
}



